<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.geekbounty.engineeringinvoice">

    <uses-sdk tools:overrideLibrary="android.support.constraint" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" />
        <activity android:name=".GroundWork">
        </activity>
        <activity android:name=".FirstActivity" />
        <activity
            android:name=".SecondActivity"
            android:label="@string/activity2_name"
            android:parentActivityName=".FirstActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.geekbounty.engineeringinvoice.FirstActivity" />
        </activity>
        <activity android:name=".ExcavatedInternalWallActivity"
            android:label="Excavated Internal Wall"/>
        <activity android:name=".StructruralCarcassingActivity"
            android:label="Structural Carcassing">
        </activity>
        <activity android:name=".WindowsDoorsActivity"></activity>
        <activity android:name=".WallFinishesActivity"
            android:label="Wall Finishes"/>
        <activity android:name=".CeilingFinishesActivity"
            android:label="Ceiling Finishes"/>
        <activity android:name=".FloorFinishesActivity"
            android:label="Floor Finishes"/>
        <activity android:name=".ElectricalServicesActivity"
            android:label="Electrical Services"/>
        <activity android:name=".MechanicalServicesActivity"
            android:label="Mechanical Services"/>
        <activity android:name=".SummaryActivity"
            android:label="Summary"/>
        <activity android:name=".HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>